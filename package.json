{
  "name": "sitematev2.0",
  "version": "1.0.0",
  "description": "SiteMate V2 an interactive rostering and timesheet system",
  "main": "index.js",
  "proxy": "http://localhost:3001",
  "engines": {
    "node": "14.16.1"
  },
  "scripts": {
    "start": "node server/server.js",
    "dev": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "seed": "cd server && npm run seed",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yurikitchin/SiteMate-V2.0.git"
  },
  "keywords": [],
